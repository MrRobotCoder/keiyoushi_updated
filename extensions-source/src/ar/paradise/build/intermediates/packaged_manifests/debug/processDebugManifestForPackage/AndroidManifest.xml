<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eu.kanade.tachiyomi.extension.ar.paradise"
    android:versionCode="37"
    android:versionName="1.4.37" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="34" />

    <uses-feature android:name="tachiyomi.extension" />

    <application
        android:allowBackup="false"
        android:debuggable="true"
        android:extractNativeLibs="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Tachiyomi: Paradise" >
        <activity
            android:name="eu.kanade.tachiyomi.multisrc.madara.MadaraUrlActivity"
            android:excludeFromRecents="true"
            android:exported="true"
            android:theme="@android:style/Theme.NoDisplay" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="paradise-bl.com"
                    android:pathPattern="/.*/..*"
                    android:scheme="https" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="tachiyomi.extension.class"
            android:value=".Paradise" />
        <meta-data
            android:name="tachiyomi.extension.nsfw"
            android:value="1" />
    </application>

</manifest>